{
  "name": "sphere-cap-random",
  "version": "1.0.1",
  "description": "Randomly sample points on the directed spherical cap",
  "main": "dist/cap-random.js",
  "module": "index",
  "jsnext:main": "index",
  "scripts": {
    "pretest": "rm -fr dist && mkdir -p dist && rollup -c",
    "test": "tape 'test/**/*-test.js'",
    "build": "npm run test && uglifyjs dist/cap-random.js -c -m -o dist/cap-random.min.js"
  },
  "keywords": [
    "random",
    "sample",
    "points",
    "sphere",
    "spherical cap",
    "geometry",
    "linear algebra"
  ],
  "author": "Ahmed Fasih",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "https://github.com/fasiha/sphere-cap-random.git"
  },
  "dependencies": {
    "iota-array": "^1.0.0",
    "ndarray": "^1.0.18",
    "ndarray-diagonal": "^1.0.0",
    "ndarray-fill": "^1.0.1",
    "ndarray-gemm": "^1.0.0",
    "ndarray-ops": "^1.2.2",
    "ndarray-pack": "^1.2.1",
    "ndarray-show": "^2.0.0",
    "ndarray-unpack": "^1.0.0",
    "zeros": "^1.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015-rollup": "^1.2.0",
    "require-reload": "^0.2.2",
    "rollup": "^0.34.10",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-browserify-transform": "^0.1.0",
    "rollup-plugin-commonjs": "^3.3.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "source-map-support": "^0.4.2",
    "tape": "^4.6.0",
    "uglify-js": "^2.7.3"
  }
}
